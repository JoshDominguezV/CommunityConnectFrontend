{
  "expo": {
    "name": "CommunityConnectFrontend",
    "slug": "CommunityConnectFrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "com.communityconnect.frontend",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.communityconnect.frontend"
    },
    "android": {
      "package": "com.communityconnect.frontend",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-web-browser",
      "expo-secure-store",
      "expo-mail-composer"
    ],
    "extra": {
      "eas": {
        "projectId": "0a72f9bf-269a-470f-adc1-f4653c55273e"
      }
    }
  }
}
